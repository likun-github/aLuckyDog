.formid-action .formid-action-button {
    padding: 0;
    margin: 0;
    border-radius: 0;
    border: none;
    font-size: inherit;
    color: inherit;
    background: inherit;
    text-align: inherit;
    line-height: inherit;
    overflow: initial;
}

.formid-action .formid-action-button:after {
    display: none;
}

.formid-action .formid-action-button:before {
    display: none;
}