/* pages/address/address.wxss */

page{
  background: #f0f0f0;
}
.navbar{
  position: relative;

}
.back-icon, .home-icon{
    width: 28px;
    height: 100%;
    position: absolute;
    transform: translateY(-50%);
    top: 50%;
    display: flex;
    background: rgba(0, 0, 0, 0);
}
.back-icon{
    left: 16px;
}
.home-icon{
    left: 44px
}
.back-icon image{
    width: 28px;
    height: 28px;
    margin: auto;
}
.home-icon image{
    width: 20px;
    height: 20px;
    margin: auto;
}
.nav-title, .nav-icon{
    position: absolute;
    transform: translate(-50%, -50%);
    left: 50%;
    top: 50%;
    font-size: 0;
    font-weight: 500;
}

.title_view{
  width: 100%;
  /* top: 0; */
  z-index: 1;
  position: fixed;
  border-bottom: 1px solid rgba(187, 187, 187, 0.24);
}

.bottom_add{
  position: absolute;
  bottom:20rpx;
  right: 20rpx;

  height: 120rpx;
  width: 120rpx;
  background: rgba(228, 228, 228, 0.897);
  border-radius: 100%;
  color: rgba(199, 146, 31, 0.795);
  font-size: 60rpx;
  font-weight: 600;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0.5rpx 0.5rpx rgba(226, 180, 82, 0.795);
  
}

.botn{
  width:90%;
  margin-left: 5%;
  height: 88rpx;
  background: rgb(231, 34, 34);
  text-align: center;
  font-size: 30rpx;
  border-radius: 50rpx;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}



.text_class_view{
  width: 100%;

  height: 500rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: white;
}

.text{

  width: 700rpx;
  height: 500rpx;
  margin-right: auto;
  margin-left: auto;
  background: white;
  font-size: 35rpx;

}